(id @factorial fun int [(int @n)] [(int @r)])
(id @main fun void [] [])

function @factorial
	t0  <- i_aload @n
	t1  <- i_value 0
	t2  <- i_eq t0, t1
	cjump t2, l0, l1
	l1 : t3 <-  i_aload @n
	t4 <-  i_value 1
	t5  <- i_eq t3, t4
	t2 <-  i_copy t5
	l0 : @c <-  i_lstore t2

function @main
	t0 <- i_value 3
	t1 <- i_call @factorial, [t0]
	i_print t1
	return