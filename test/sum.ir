(id @sum fun int [(int @m) (int @n) (int @s)] [(int @t)])
(id @main fun void [] [])

function @sum
	t0 <- i_aload @m
	t1 <- i_value 0
	t2 <- i_eq t0, t1
	cjump t2, l0, l1
l0:	t3 <- i_value 2
	t4 <- i_value 3
	t5 <- i_value 4
	t6 <- i_call @sum, [t3,t4,t5]
	@t <- i_lstore t6
l1:	t7 <- i_aload @m
	t8 <- i_value 1
	t9 <- i_sub t7, t8
	t10 <- i_aload @n
	t11 <- i_value 1
	t12 <- i_sub t10, t11
	t13 <- i_aload @s
	t14 <- i_value 2
	t15 <- i_sub t13, t14
	t16 <- i_call @sum, [t9,t12,t15]
	@t <- i_lstore t16
	t17 <- i_lload @t
	i_return t17

function @main
	t0 <- i_value 1
	t1 <- i_value 2
	t2 <- i_value 3
	t3 <- i_call @sum, [t0,t1,t2]
	i_print t3
	return
